<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>


<VBox prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.managerandreminder.controllers.registerController">


   <Label fx:id="usernameText" text="username" textAlignment="CENTER" />

   <TextField fx:id="inputUsername" promptText="Username" />

   <Label fx:id="passwordText" text="password" textAlignment="CENTER" />

   <TextField fx:id="inputPassword" promptText="Password" />

   <Label fx:id="emailText" text="email" textAlignment="CENTER" />

   <TextField fx:id="inputEmail" promptText="email" />


   <Label fx:id="errorText" />

   <Button text="register" onAction="#onSignin" />

</VBox>
